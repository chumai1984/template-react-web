{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": false,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": false,
    "baseUrl": ".",
    "noUnusedLocals": true,
    "allowJs": true,
    "strict": false,
    "paths": {
      "@/*": ["src/*"],
      "@tmp/*": ["src/pages/.umi/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "build",
  ]
}
